 


<div class="col-md-12 container"  	>
<div class="row">

<h1 class="text-center"> Welcome Client</h1>
<div class="col-md-4">
<div class="border-form-div">
  <h2>Sign in</h2>
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
    <%= f.email_field :email, :autofocus => true ,:placeholder => 'Email address', :class=>'form-control' %>
    <%= f.password_field :password , :placeholder => 'Password' , :class=>'form-control' %>
 
    <%= f.submit "Sign in" , :class=>'btn btn-primary'%>
  <% end %>
  <%= render "devise/shared/links" %>
   
</div>
</div>

<div class="col-md-4">
</div>


<div class="col-md-4">
<div class="border-form-div">
  <h2>Sign up</h2>
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
    <%= f.email_field :email, :autofocus => true, :placeholder => 'Email address'  , :class=>'form-control'%>
    <%= f.text_field :name, :placeholder => 'Name' , :class=>'form-control'%>
    <%= f.text_field :phone, :placeholder => 'Phone' , :class=>'form-control'%>
    <%= f.text_field :mobile, :placeholder => 'Mobile' , :class=>'form-control'%>
    <%= f.text_field :address, :placeholder => 'Address' , :class=>'form-control'%>
    <%= f.text_field :district, :placeholder => 'District' , :class=>'form-control'%>
    <%= f.text_field :state, :placeholder => 'State' , :class=>'form-control'%>
    <%= f.text_field :pin, :placeholder => 'Pin' , :class=>'form-control'%>
    <%= f.password_field :password, :placeholder => 'Password' , :class=>'form-control' %>
    <%= f.password_field :password_confirmation, :placeholder => 'Password confirmation' , :class=>'form-control'%>
    <%= f.submit "Sign up",:class=>'btn btn-primary' %>
  <% end %>
</div>


</div>
</div>
</div>





 


